{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["build/**", "dist/**"]
		},
		"check-exports": {
			"dependsOn": ["^check-exports"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"dependsOn": ["^dev"],
			"cache": false,
			"persistent": true
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"nuke": {
			"dependsOn": ["^nuke"]
		},
		"start": {
			"dependsOn": ["^start"]
		},
		"test": {
			"dependsOn": ["^build"]
		},
		"test:coverage": {
			"dependsOn": ["^build"]
		}
	}
}
