{
	"name": "@kobun/core",
	"version": "0.6.0",
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"style": "./dist/index.css",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./kobun.css": "./dist/index.css"
	},
	"scripts": {
		"build": "pnpm run build:css && pnpm run build:lib",
		"build:css": "tailwindcss -m -i ./src//index.css -o dist/index.css",
		"build:lib": "tsup",
		"dev": "concurrently \"pnpm run dev:css\" \"pnpm run dev:lib\"",
		"dev:css": "tailwindcss -w -i ./src/index.css -o dist/index.css",
		"dev:lib": "tsup --watch --dts",
		"nuke": "rimraf dist node_modules .turbo"
	},
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0",
		"react-router": "^7.0.0"
	},
